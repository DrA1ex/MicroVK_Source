<UserControl x:Class="ControllUtilsList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:loc="clr-namespace:MicroVK.My.Resources"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             d:DesignHeight="300"
             d:DesignWidth="400"
             mc:Ignorable="d">
    <Grid>
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <mui:LinkCollection>
                    <mui:Link Source="/Content/Utils/ControlCheckLink.xaml" DisplayName="{x:Static loc:Resources.utils_checkLink}" />
                </mui:LinkCollection>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
